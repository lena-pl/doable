<h1>Users</h1>

<%= link_to new_user_path, class: "btn btn-default" do %>
    <i class="glyphicon glyphicon-plus"></i> New User
<% end %>

<% @users.each do |user| %>
    <h4><%= user.email %></h4>
    <p><%= link_to 'Show Profile', user %></p>
    <p><%= link_to 'Edit', edit_user_path(user) %>
       <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></p>
<% end %>

<br>
